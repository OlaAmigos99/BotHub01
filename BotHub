local Tab = Window:MakeTab({
	Name = "Tab main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
  Name = "Recuperar Vida!",
  Callback = function()
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChild("Humanoid") then
      local humanoid = player.Character.Humanoid
      while wait(1) do
        humanoid.Health = humanoid.MaxHealth
      end
    end
  end    
})

Tab:AddTextbox({
  Name = "aumenta volocidade",
  Default = "",
  TextDisappear = true,
  Callback = function(Value)
    local speed = tonumber(Value)
    if speed and speed > 0 then
      local player = game.Players.LocalPlayer
      if player and player.Character and player.Character:FindFirstChild("Humanoid") then
        local humanoid = player.Character.Humanoid
        humanoid.WalkSpeed = speed
      end
    else
      print("Por favor, insira um número válido para a velocidade.")
    end
  end	  
})

Tab:AddTextbox({
  Name = "aumenta JumpPower",
  Default = "",
  TextDisappear = true,
  Callback = function(Value)
    local jumpPower = tonumber(Value)
    if jumpPower and jumpPower > 0 then
      local player = game.Players.LocalPlayer
      if player and player.Character and player.Character:FindFirstChild("Humanoid") then
        local humanoid = player.Character.Humanoid
        humanoid.JumpPower = jumpPower
      end
    else
      print("Por favor, insira um número válido para o poder de salto.")
    end
  end	  
})

local ESPEnabled = false

local function CreateESP(player)
  for _, part in pairs(player.Character:GetChildren()) do
    if part:IsA("BasePart") then
      local highlight = Instance.new("BoxHandleAdornment")
      highlight.Adornee = part
      highlight.AlwaysOnTop = true
      highlight.ZIndex = 10
      highlight.Size = part.Size
      highlight.Color3 = Color3.new(0, 1, 0)
      highlight.Parent = part
    end
  end
end

local function RemoveESP(player)
  for _, part in pairs(player.Character:GetChildren()) do
    if part:IsA("BasePart") then
      for _, adornment in pairs(part:GetChildren()) do
        if adornment:IsA("BoxHandleAdornment") then
          adornment:Destroy()
        end
      end
    end
  end
end

Tab:AddToggle({
  Name = "ESP Player",
  Default = false,
  Callback = function(Value)
    ESPEnabled = Value
    if ESPEnabled then
      for _, player in pairs(game.Players:GetPlayers()) do
        if player.Character then
          CreateESP(player)
        end
      end
    else
      for _, player in pairs(game.Players:GetPlayers()) do
        if player.Character then
          RemoveESP(player)
        end
      end
    end
  end    
})

game.Players.PlayerAdded:Connect(function(player)
  if ESPEnabled then
    player.CharacterAdded:Connect(function()
      CreateESP(player)
    end)
  end
end)

game.Players.PlayerRemoving:Connect(function(player)
  if ESPEnabled then
    RemoveESP(player)
  end
end)

