local Tab = Window:MakeTab({
	Name = "Tab Gui",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Gui de cordenadas (Beta)",
	Callback = function()
   local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local CoordinatesLabel = Instance.new("TextLabel")
local CopyButton = Instance.new("TextButton")
local DeleteButton = Instance.new("TextButton")
local CloseButton = Instance.new("TextButton")

ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.new(1, 1, 1)
Frame.Size = UDim2.new(0, 200, 0, 100)
Frame.Position = UDim2.new(0.5, -100, 0.5, -50)

CoordinatesLabel.Parent = Frame
CoordinatesLabel.BackgroundColor3 = Color3.new(1, 1, 1)
CoordinatesLabel.Size = UDim2.new(1, 0, 0.5, 0)
CoordinatesLabel.Text = "Coordenadas:"

local function updateCoordinates()
    local player = game.Players.LocalPlayer
    local humanoidRootPart = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
    if humanoidRootPart then
        local position = humanoidRootPart.Position
        CoordinatesLabel.Text = string.format("Coordenadas: X: %f, Y: %f, Z: %f", position.X, position.Y, position.Z)
    end
end

CopyButton.Parent = Frame
CopyButton.Text = "Copiar"
CopyButton.Size = UDim2.new(0.5, 0, 0.5, 0)
CopyButton.Position = UDim2.new(0, 0, 0.5, 0)
CopyButton.MouseButton1Click:Connect(function()
    setclipboard(CoordinatesLabel.Text)
end)

DeleteButton.Parent = Frame
DeleteButton.Text = "Excluir"
DeleteButton.Size = UDim2.new(0.5, 0, 0.5, 0)
DeleteButton.Position = UDim2.new(0.5, 0, 0.5, 0)
DeleteButton.MouseButton1Click:Connect(function()
    CoordinatesLabel.Text = "Coordenadas:"
end)

CloseButton.Parent = ScreenGui
CloseButton.Text = "X"
CloseButton.Size = UDim2.new(0, 50, 0, 50)
CloseButton.Position = UDim2.new(0.5, 150, 0.5, -25)
CloseButton.BackgroundColor3 = Color3.new(1, 0, 0)
CloseButton.MouseButton1Click:Connect(function()
    ScreenGui:Destroy()
end)

while true do
    wait(1)
    updateCoordinates()
end

  	end    
})


local Tab = Window:MakeTab({
	Name = "Tab troll",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local pullPlayersEnabled = false

local function PullPlayers()
  local localPlayerPosition = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
  while pullPlayersEnabled do
    for _, player in pairs(game.Players:GetPlayers()) do
      if player ~= game.Players.LocalPlayer then
        player.Character.HumanoidRootPart.CFrame = localPlayerPosition
      end
    end
    wait(1)
  end
end

Tab:AddToggle({
  Name = "Puxar Jogadores",
  Default = false,
  Callback = function(Value)
    pullPlayersEnabled = Value
    if pullPlayersEnabled then
      PullPlayers()
    end
  end    
})

 local noclipEnabled = false

local function NoclipLoop()
  while noclipEnabled do
    if game.Players.LocalPlayer.Character then
      for _, part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
        if part:IsA("BasePart") then
          part.CanCollide = false
        end
      end
    end
    game:GetService("RunService").Stepped:wait()
  end
end

Tab:AddToggle({
  Name = "Noclip",
  Default = false,
  Callback = function(Value)
    noclipEnabled = Value
    if noclipEnabled then
      NoclipLoop()
    else
      if game.Players.LocalPlayer.Character then
        for _, part in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
          if part:IsA("BasePart") then
            part.CanCollide = true
          end
        end
      end
    end
  end    
})

Tab:AddButton({
    Name = "Teleportar para Jogador Aleatório",
    Callback = function()
        local players = game:GetService("Players")
        local localPlayer = players.LocalPlayer
        local allPlayers = players:GetPlayers()
        local randomPlayer = allPlayers[math.random(1, #allPlayers)]

        if randomPlayer and randomPlayer.Character and randomPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local randomPlayerPosition = randomPlayer.Character.HumanoidRootPart.Position
            if localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") then
                localPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(randomPlayerPosition)
            end
        else
            print("Não foi possível encontrar um jogador válido para teleportar.")
        end
    end    
})

local teleportToggle = false
local teleportDelay = 1

Tab:AddToggle({
    Name = "Teleporta para um jogador automaticamente",
    Default = false,
    Callback = function(Value)
        teleportToggle = Value
        if teleportToggle then
            print("Teleporte Aleatório ativado.")
        else
            print("Teleporte Aleatório desativado.")
        end
    end    
})

while true do
    wait(teleportDelay)
    if teleportToggle then
        local players = game:GetService("Players")
        local localPlayer = players.LocalPlayer
        local allPlayers = players:GetPlayers()
        local randomPlayer = allPlayers[math.random(1, #allPlayers)]

        if randomPlayer and randomPlayer.Character and randomPlayer.Character:FindFirstChild("HumanoidRootPart") and localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") then
            localPlayer.Character.HumanoidRootPart.CFrame = randomPlayer.Character.HumanoidRootPart.CFrame
        end
    end
end
